apiVersion: k8s.cni.cncf.io/v1
kind: NetworkAttachmentDefinition
metadata:
  name: ovs-vlan-b
  namespace: {{ paa_namespace }}
  annotations:
    k8s.v1.cni.cncf.io/resourceName: {{ ovs_cni_resource }}
spec:
  config: |
    {
      "cniVersion": "0.3.1",
      "name": "ovs-vlan-b",
      "plugins": [
        {
          "type": "ovs",
          "vlan": {{ vlan_b }},
          "bridge": "{{ ovs_bridge }}",
          "ipam": {
            "type": "whereabouts",
            "range": "{{ ipam_b_cidr }}",
            "exclude": {{ ipam_excludes | to_json }}
          }
        }
      ]
    }
